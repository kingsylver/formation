			<!-- Registration -->

            <!--Tree view-->
            <record model="ir.ui.view" id="registration_tree_view">
                <field name="name">registration.registration.tree</field>
                <field name="model">registration.registration</field>
                <field name="type">tree</field>
                <field name="arch" type="xml">
                    <tree string="Inscription">
                        <field name="name"/>
                        <field name="code"/>
                        <field name="start_date"/>
                        <field name="end_date"/>
                        <field name="cycle_id"/>
                        <field name="year_id"/>
                    </tree>
                </field>
            </record>
            
			<!--form view-->
            <record model="ir.ui.view" id="registration_form_view">
                <field name="name">registration.registration.form</field>
                <field name="model">registration.registration</field>
                <field name="type">form</field>
                <field name="arch" type="xml">
                    <form string="Inscription">
                    <header>
                       <button name="action_new"  string="Nouveau" type="object" class="oe_highlight"/>
                       <button name="action_done" states="cancel" string="Valider" type="object" class="oe_highlight"/>
                       <button name="action_cancel" states="new" string="Annuler" type="object" class="oe_highlight"/>
	                   <field name="state" widget="statusbar" />
               		 </header>
                     <sheet>
                     	 <div class="oe_button_box" name="button_box">
	                        <button name="%(claim_action_view)d"
                                type="action"
                                class="oe_inline oe_stat_button"
                                icon="fa-book"
                                context="{'search_default_regist_id': active_id}"
                                help="show the information for this">
                               <field name="nb_reclam" widget="statinfo" string="Reclamation"/>
                            </button>   
	                      </div>
                    	<group>
	                    	<group>
	                    		<field name="name"/>
	                    		<field name="start_date"/>
                        		<field name="end_date"/>
                        		<field name="etudiant"/>
	                    	</group>
	                    	<group>
	                    		<field name="code"/>
	                    		<field name="cycle_id"/>
                        		<field name="year_id"/>
                        		<!--  field name="state"/ -->
                        		<!-- field name="nb_reclam"/ -->
	                    	</group>
                    	</group>
                    	<notebook>
                            <page string="Reclamation">
                                <field name="claims_ids"/>
                            </page>
                        </notebook>
                    </sheet>
                     <div class="oe_chatter">
	                    <field name="message_follower_ids" widget="mail_followers"/>
	                    <field name="message_ids" widget="mail_thread"/>
                	</div>
                   </form>
                </field>
            </record>
			
