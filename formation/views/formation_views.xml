  	<!-- Reclamation -->
 	
            <!--Tree view-->
            <record model="ir.ui.view" id="claim_tree_view">
                <field name="name">claim.claim.tree</field>
                <field name="model">claim.claim</field>
                <field name="type">tree</field>
                <field name="arch" type="xml">
                    <tree string="Reclamation">
                        <field name="name"/>
                        <field name="code"/>
                        <field name="regist_id"/>
                        <field name="description"/>
                    </tree>
                </field>
            </record>
            
			<!--form view-->
            <record model="ir.ui.view" id="claim_form_view">
                <field name="name">claim.claim.form</field>
                <field name="model">claim.claim</field>
                <field name="type">form</field>
                <field name="arch" type="xml">
                    <form string="Reclamation">
                     <sheet>
                    	<group>
	                    	<group>
	                    		<field name="name"/>
	                    		<field name="start_date"/>
	                    		<field name="regist_id"/>
	                    		<field name="state"/>
	                    		
	                    		<!--field name="nb_hour"/-->
	                    		<field name="amount"/>
	                    		<field name="total"/>
	                    	</group>
	                    	<group>
	                    		<field name="code"/>
	                    		<field name="user_id"/>
	                    		<field name="end_date"/>
	                    		<field name="priority" widget="priority"/>
	                    	</group>
                    	</group>
                    	<notebook>
                            <page string="Description">
                                <field name="description"/>
                            </page>
                        </notebook>
                    </sheet>
                   </form>
                </field>
            </record>
            
            <!--claim.claim:calendar view-->
			<record model="ir.ui.view" id="claim_calendar_view">
			    <field name="name">claim.claim.calendar</field>
			    <field name="model">claim.claim</field>
			    <field name="priority" eval="2"/>
			    <field name="arch" type="xml">
			    	<calendar string="Réclamation" date_start="start_date" color="user_id">
			             <field name="name"/>
			             <field name="code"/>
			        </calendar>
			    </field>
			</record>
					
			  <!-- claim.claim: Search views -->
		       <record id="claim_request_view_search" model="ir.ui.view">
		        <field name="name">claim.claim.search</field>
		        <field name="model">claim.claim</field>
		        <field name="arch" type="xml">
		            <search string="DescriptionSearch">
		                <field name="name"/>
		                <field name="code"/>
		                <field name="regist_id"/>
		                <separator/>
		                <filter string="Nouvelle" name="new" domain="[('state', '=', 'new')]"/>
		                <filter string="Validé" name="done" domain="[('state', '=', 'done')]"/>
		                <filter string="Cancel" name="cancel" domain="[('state', '=', 'cancel')]"/>
		                <group  expand='0' string='Group by...'>
		                    <filter string='Responsable' domain="[]" context="{'group_by': 'user_id'}"/>
		                    <filter string='Inscription' domain="[]" context="{'group_by' : 'regist_id'}"/>
		                </group>
		            </search>
		        </field>
		    </record>
			    
			    <!-- View Graph -->
			    <record model="ir.ui.view" id="claim_graph_view">
						<field name="name">claim.claim.graph</field>
						<field name="model">claim.claim</field>
						<field name="arch" type="xml">
							<graph string="Graphe">
								<field name="name" />
								<field name="state" />
								<field name="user_id" />
							</graph>
						</field>
			</record>
			
			<!-- Pivot view claim -->
		    <record id="claim_view_pivot" model="ir.ui.view">
		        <field name="name">claim.claim.pivot</field>
		        <field name="model">claim.claim</field>
		        <field name="arch" type="xml">
		            <pivot string="Réclamtion Pivot">
		                <field name="name"/>
						<field name="state" />
						<field name="user_id" />
		            </pivot>
		        </field>
		    </record>
			
		<!-- claim.claim :view kanban -->
		<record model="ir.ui.view" id="claim_kanban_view">
			<field name="name">claim.claim.Kanban</field>
			<field name="model">claim.claim</field>
			<field name="arch" type="xml">
				<kanban>
					<templates>
						<field name="name" />
						<field name="regist_id"/>
						<field name="description"/>
						<t t-name="kanban-box">
		                    <div class="oe_kanban_global_click">
		                        <field name="name"/>
		                        <div class="oe_kanban_details">
		                            <strong>
		                                <field name="code"/>
		                            </strong>
		                            <ul>
		                                <li t-if="record.name.raw_value"><field name="start_date"/></li>
		                                <li t-if="record.regist_id.raw_value"><i class="fa fa-phone"></i> <field name="regist_id"/></li>
		                                <li t-if="record.description.raw_value"><field name="description"/></li>
		                                <li t-if="record.priority.raw_value"><field name="priority" widget="priority"/></li>
		                            </ul>
		                        </div>
		                    </div>
                    	</t>
					</templates>
				</kanban>
			</field>
		</record>
		
			<!-- Action view-->
            <record model="ir.actions.act_window" id="claim_action_view">
                <field name="name">Reclamation</field>
                <field name="type">ir.actions.act_window</field>
                <field name="res_model">claim.claim</field>
                <field name="view_type">form</field>
                <field name="view_mode">kanban,tree,form,calendar,pivot,graph</field>
                <!-- <field name="domain">[('field_name','condition',criteria)]</field>-->
                <!-- <field name="context">{'default_field':'value'}</field>-->
            </record>
		   


