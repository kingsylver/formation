			<record model="ir.ui.view" id="student_inh_partner_view">
			    <field name="name">student.partner.form</field>
			    <field name="model">res.partner</field>
			    <field name="inherit_id" ref="base.view_partner_form"/>
			    <field name="arch" type="xml">
			        <data>
			            <xpath expr="//page[@name='internal_notes']" position="after">
			            <page string="Infos Etudiant">
				            <group>
				                <field name="student_ok"/>
				                <field name="birthday"/>
				                <field name="age" attrs="{'readonly':[('student_ok','=',True)]}" />
				                <!-- field name="reg_ids"/ -->
				            </group>
			             </page>
			            </xpath>
			            <xpath expr="//page[@name='sales_purchases']" position="after">
			            <page string="Inscription" attrs="{'invisible':[('customer','=',True)]}">
				                <field name="reg_ids" string=""/>
			             </page>
			            </xpath>
			                    
			    		<xpath expr="//page[@name='sales_purchases']" position="attributes">
			            	<attribute name='attrs'>{"invisible":[('student_ok','=',True)]}</attribute>
			            </xpath>
	
			        </data>
			    </field>
			</record>

